{
  "name": "notify-cli",
  "version": "1.0.0",
  "description": "A CLI app to receive notifications when AFK",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "notify": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["cli", "notifications", "telegram", "effect"],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@effect/cli": "^0.73.0",
    "@effect/platform": "^0.94.1",
    "@effect/platform-node": "^0.104.0",
    "@effect/schema": "^0.75.5",
    "effect": "^3.19.14"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
